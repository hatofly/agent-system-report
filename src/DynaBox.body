format: ChoreonoidBody
formatVersion: 1.0
angleUnit: degree
name: DynaBox

links:
# MARK:外殻
  - 
    name: CENTER
    root: true
    translation: [0, 0, 1]
    jointType: free
    # centerOfMass: [0, 0, 0]
    # mass: 0.1
    # inertia: [
    #   0.00017, 0,   0,
    #   0,   0.00017, 0,
    #   0,   0,   0.00017 ]
    elements:
      type: RateGyroSensor
      name: GYRO
      id: 0
    
  - 
    name: BOTTOM_PLATE
    parent: CENTER
    translation: [0, 0, -0.95]
    jointType: fixed
    centerOfMass: [0, 0, 0]
    mass: &PLATEMASS 0.1
    inertia: &SHORTPLATEINERTIA [
      0.003, 0,   0,
      0,   0.003, 0,
      0,   0,   0.006 ]
    elements:
      Shape:
        geometry: &SHORTPLATE
          type: Box
          size: [0.9, 0.9, 0.05]
        appearance: &PlateAppearance
          material:
            diffuseColor: [0, 0.6, 0.6]
            transparency: 0.8
            shininess: 0.6

  # - 
  #   name: ACCEL
  #   type: ACCEL_SENSOR
  #   id: 0
  - 
    name: TOP_PLATE
    parent: CENTER
    translation: [0, 0, 0.95]
    jointType: fixed
    centerOfMass: [0, 0, 0]
    mass: *PLATEMASS
    inertia: *SHORTPLATEINERTIA
    elements:
      Shape:
        geometry: *SHORTPLATE
        appearance: *PlateAppearance
  - 
    name: FRONT_PLATE
    parent: CENTER
    translation: [0.45 , 0, 0]
    jointType: fixed
    centerOfMass: [0, 0, 0]
    mass: *PLATEMASS
    inertia: &LONGPLATEINERTIA [
      0.024, 0, 0,
      0, 0.021, 0,
      0, 0, 0.003
    ]
    elements:
      Shape:
        geometry: &LONGPLATE
          type: Box
          size: [0.05, 0.9, 1.9]
        appearance: *PlateAppearance
  - 
    name: BACK_PLATE
    parent: CENTER
    translation: [-0.45 , 0, 0]
    jointType: fixed
    centerOfMass: [0, 0, 0]
    mass: *PLATEMASS
    inertia: *LONGPLATEINERTIA
    elements:
      Shape:
        geometry: *LONGPLATE
        appearance: *PlateAppearance
  - 
    name: LEFT_PLATE
    parent: CENTER
    translation: [0 , 0.45, 0]
    rotation: [0,0,1, 90]
    jointType: fixed
    centerOfMass: [0, 0, 0]
    mass: *PLATEMASS
    inertia: *LONGPLATEINERTIA
    elements:
      Shape:
        geometry: *LONGPLATE
        appearance: *PlateAppearance
  - 
    name: RIGHT_PLATE
    parent: CENTER
    translation: [0 , -0.45, 0]
    rotation: [0,0,1, 90]
    jointType: fixed
    centerOfMass: [0, 0, 0]
    mass: *PLATEMASS
    inertia: *LONGPLATEINERTIA
    elements:
      Shape:
        geometry: *LONGPLATE
        appearance: *PlateAppearance


